    "version": "0.2.0",
    "configurations": [
        {
            "type": "node-terminal",
            "name": "Launch MCP:http",
            "request": "launch",
            "command": "npm run start:http",
            "cwd": "${workspaceFolder}\\fhir-jira-mcp"
        },
        {
            "type": "node-terminal",
            "name": "Create FTS",
            "request": "launch",
            "command": "npm run create-fts",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Create TFIDF",
            "request": "launch",
            "command": "npm run create-tfidf",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Extract Archives",
            "request": "launch",
            "command": "npm run extract-archives",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Load Initial",
            "request": "launch",
            "command": "node extract-xml\\load-initial.js",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Load Updates",
            "request": "launch",
            "command": "node extract-xml\\load-updates.js",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Test TFIDF",
            "request": "launch",
            "command": "node test-tfidf.js",
            "cwd": "${workspaceFolder}\\extract-xml"
        },
        {
            "type": "node-terminal",
            "name": "TFIDF Processor",
            "request": "launch",
            "command": "node tfidf-processor.js",
            "cwd": "${workspaceFolder}\\extract-xml"
        }
    ]
}